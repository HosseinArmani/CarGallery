@model IEnumerable<Car.DataLayer.Entities.Agency>

<h3>نمایندگی ها</h3>

<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item d-flex justify-content-between align-items-center">
            <a href="/Home/Agency/@item.AgencyId">@item.Name</a>

        </li>
    }
</ul>


