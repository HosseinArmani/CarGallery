@model IEnumerable<Car.DataLayer.Entities.CarGroup>

<h3>گروه ها</h3>

<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item d-flex justify-content-between align-items-center">
            <a href="/Home/Archive?SelectGroups=@item.GroupId">@item.TitleGroup</a>
           
        </li>
    }
</ul>
